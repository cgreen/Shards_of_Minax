  <SpeechEntry>
    <EntryNumber>0</EntryNumber>
    <ID>0</ID>
    <Text>I am Samuel de Champlain, a traveler from the distant land of France.</Text>
    <Keywords>name</Keywords>
    <DependsOn>-1</DependsOn>
