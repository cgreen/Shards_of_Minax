  <SpeechEntry>
    <EntryNumber>60</EntryNumber>
    <ID>60</ID>
    <Text>Ah, you've heard of me? They say I've the strength of ten men and the speed of a panther!</Text>
    <Keywords>brawn</Keywords>
    <DependsOn>-1</DependsOn>
    <Pause>1</Pause>
