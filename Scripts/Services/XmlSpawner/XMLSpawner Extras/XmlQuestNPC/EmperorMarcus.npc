  <SpeechEntry>
    <EntryNumber>10</EntryNumber>
    <ID>10</ID>
    <Text>Emperor Marcus scoffs, "Health? What's it to you?"</Text>
    <Keywords>health</Keywords>
    <DependsOn>-1</DependsOn>
