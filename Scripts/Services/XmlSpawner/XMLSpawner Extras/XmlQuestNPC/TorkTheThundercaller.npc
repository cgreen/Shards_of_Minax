  <SpeechEntry>
    <EntryNumber>10</EntryNumber>
    <ID>10</ID>
    <Text>I am in perfect health, untouched by the elements.</Text>
    <Keywords>health</Keywords>
    <DependsOn>-1</DependsOn>
