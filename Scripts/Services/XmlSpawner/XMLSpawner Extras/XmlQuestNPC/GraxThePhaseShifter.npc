    <AllowNPCTrigger>False</AllowNPCTrigger>
    <SpeechStyle>Regular</SpeechStyle>
    <SpeechHue>-1</SpeechHue>
  </SpeechEntry>
  <SpeechEntry>
    <EntryNumber>40</EntryNumber>
    <ID>40</ID>
    <Text>Ha! You know nothing of my world. Begone, before I shift you into oblivion!</Text>
    <Keywords>oblivion</Keywords>
    <DependsOn>30</DependsOn>
    <Pause>1</Pause>
