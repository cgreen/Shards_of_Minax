  <SpeechEntry>
    <EntryNumber>10</EntryNumber>
    <ID>10</ID>
    <Text>I am in perfect health, as the land itself heals my wounds.</Text>
    <Keywords>health</Keywords>
    <DependsOn>-1</DependsOn>
