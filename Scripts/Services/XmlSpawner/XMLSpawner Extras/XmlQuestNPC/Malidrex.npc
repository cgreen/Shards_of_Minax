    <AllowNPCTrigger>False</AllowNPCTrigger>
    <SpeechStyle>Regular</SpeechStyle>
    <SpeechHue>-1</SpeechHue>
  </SpeechEntry>
  <SpeechEntry>
    <EntryNumber>360</EntryNumber>
    <ID>360</ID>
    <Text>With strong resolve, even the toughest challenges become surmountable. It's the fire within that propels us forward.</Text>
    <Keywords>resolve</Keywords>
    <DependsOn>-1</DependsOn>
    <Pause>1</Pause>
    <PrePause>-1</PrePause>
