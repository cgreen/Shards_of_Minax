  <SpeechEntry>
    <EntryNumber>10</EntryNumber>
    <ID>10</ID>
    <Text>My existence is a curse, much like your mortal lives.</Text>
    <Keywords>health</Keywords>
    <DependsOn>-1</DependsOn>
