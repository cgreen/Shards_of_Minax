  <SpeechEntry>
    <EntryNumber>10</EntryNumber>
    <ID>10</ID>
    <Text>I exist beyond the realms of flesh and bone, my health is irrelevant.</Text>
    <Keywords>health</Keywords>
    <DependsOn>-1</DependsOn>
