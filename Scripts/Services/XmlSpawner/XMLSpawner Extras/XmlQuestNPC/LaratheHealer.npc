  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>I heal those in need and guide souls seeking wisdom.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
