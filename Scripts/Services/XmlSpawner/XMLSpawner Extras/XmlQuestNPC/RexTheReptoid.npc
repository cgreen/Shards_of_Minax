  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>I am a cosmic observer, contemplating the virtues of your world.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
