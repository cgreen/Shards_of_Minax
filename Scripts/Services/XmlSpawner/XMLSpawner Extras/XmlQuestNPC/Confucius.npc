  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>I am but a humble philosopher, seeking the path of wisdom.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
