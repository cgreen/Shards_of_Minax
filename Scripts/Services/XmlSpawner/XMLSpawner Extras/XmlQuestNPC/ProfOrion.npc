  <SpeechEntry>
    <EntryNumber>0</EntryNumber>
    <ID>0</ID>
    <Text>I am Professor Orion, a brilliant scientist, or so they say...</Text>
    <Keywords>name</Keywords>
    <DependsOn>-1</DependsOn>
