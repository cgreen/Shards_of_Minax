  <SpeechEntry>
    <EntryNumber>10</EntryNumber>
    <ID>10</ID>
    <Text>Ah, my health? The laughter keeps me hearty and hale!</Text>
    <Keywords>health</Keywords>
    <DependsOn>-1</DependsOn>
