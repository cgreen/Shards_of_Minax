  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>My occupation is that of a scientist. I study the mysteries of the world.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
