  <SpeechEntry>
    <EntryNumber>40</EntryNumber>
    <ID>40</ID>
    <Text>Do you possess the virtue of compassion, my friend?</Text>
    <Keywords>virtue compassion yes no</Keywords>
    <DependsOn>30</DependsOn>
