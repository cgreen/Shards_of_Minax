  <SpeechEntry>
    <EntryNumber>10</EntryNumber>
    <ID>10</ID>
    <Text>Life on the streets, friend, it's a constant battle.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
