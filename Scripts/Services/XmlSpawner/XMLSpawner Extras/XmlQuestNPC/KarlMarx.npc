  <SpeechEntry>
    <EntryNumber>10</EntryNumber>
    <ID>10</ID>
    <Text>My health is not relevant; what matters is the health of society.</Text>
    <Keywords>health</Keywords>
    <DependsOn>-1</DependsOn>
