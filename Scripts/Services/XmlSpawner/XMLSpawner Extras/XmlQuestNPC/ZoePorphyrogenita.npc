  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>I am a scholar of Byzantium, dedicated to the pursuit of knowledge.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
