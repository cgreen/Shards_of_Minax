  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>My job is to spread tales and melodies throughout the land.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
