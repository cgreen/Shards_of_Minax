  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>I guide souls to the path of truth and enlightenment.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
