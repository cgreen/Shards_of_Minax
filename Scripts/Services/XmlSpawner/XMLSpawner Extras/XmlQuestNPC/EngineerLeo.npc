  <SpeechEntry>
    <EntryNumber>40</EntryNumber>
    <ID>40</ID>
    <Text>Power is nothing without knowledge. Do you seek knowledge?</Text>
    <Keywords>power</Keywords>
    <DependsOn>30</DependsOn>
