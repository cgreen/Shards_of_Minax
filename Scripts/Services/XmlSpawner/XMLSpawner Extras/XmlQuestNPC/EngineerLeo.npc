  <SpeechEntry>
    <EntryNumber>10</EntryNumber>
    <ID>10</ID>
    <Text>My health is inconsequential. What matters is the pursuit of knowledge!</Text>
    <Keywords>health</Keywords>
    <DependsOn>-1</DependsOn>
