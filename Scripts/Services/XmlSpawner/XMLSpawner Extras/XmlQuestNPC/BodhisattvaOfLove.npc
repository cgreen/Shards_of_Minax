  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>I guide souls in understanding true love and compassion.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
