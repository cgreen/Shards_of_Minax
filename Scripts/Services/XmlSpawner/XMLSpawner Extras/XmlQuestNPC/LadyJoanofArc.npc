  <SpeechEntry>
    <EntryNumber>10</EntryNumber>
    <ID>10</ID>
    <Text>My health is quite robust, for I strive to maintain a body and spirit in harmony.</Text>
    <Keywords>health</Keywords>
    <DependsOn>-1</DependsOn>
