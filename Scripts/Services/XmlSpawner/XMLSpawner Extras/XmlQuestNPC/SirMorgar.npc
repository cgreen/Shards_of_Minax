  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>I am an assassin, a master of shadows and death.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
