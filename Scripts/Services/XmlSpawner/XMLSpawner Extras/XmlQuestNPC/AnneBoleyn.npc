  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>My daily duty is to tend to the gardens of this fine estate.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
