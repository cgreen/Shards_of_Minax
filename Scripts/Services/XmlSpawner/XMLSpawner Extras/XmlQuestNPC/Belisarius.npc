  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>I am a seeker of ancient mysteries and keeper of forgotten knowledge.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
