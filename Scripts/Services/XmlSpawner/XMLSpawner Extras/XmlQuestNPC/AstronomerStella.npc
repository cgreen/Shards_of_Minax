  <SpeechEntry>
    <EntryNumber>0</EntryNumber>
    <ID>0</ID>
    <Text>Greetings, traveler. I am Astronomer Stella, a seeker of celestial truths.</Text>
    <Keywords>name</Keywords>
    <DependsOn>-1</DependsOn>
