  <SpeechEntry>
    <EntryNumber>60</EntryNumber>
    <ID>60</ID>
    <Text>Lyria is an ancient name, passed down in my family. It means "whisper" in the old tongue.</Text>
    <Keywords>lyria</Keywords>
    <DependsOn>-1</DependsOn>
    <Pause>1</Pause>
