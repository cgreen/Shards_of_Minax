  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>My occupation is to seek the secrets of the universe.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
