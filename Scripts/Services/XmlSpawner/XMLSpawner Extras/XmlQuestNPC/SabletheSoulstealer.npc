  <SpeechEntry>
    <EntryNumber>0</EntryNumber>
    <ID>0</ID>
    <Text>I am Sable the Soulstealer, master of the dark arts.</Text>
    <Keywords>name</Keywords>
    <DependsOn>-1</DependsOn>
