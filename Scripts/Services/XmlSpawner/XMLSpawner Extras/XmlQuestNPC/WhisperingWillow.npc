  <SpeechEntry>
    <EntryNumber>50</EntryNumber>
    <ID>50</ID>
    <Text>My ancestors named me after an ancient tree that stands in the heart of our village. It's believed to have magical properties.</Text>
    <Keywords>willow</Keywords>
    <DependsOn>-1</DependsOn>
    <Pause>1</Pause>
