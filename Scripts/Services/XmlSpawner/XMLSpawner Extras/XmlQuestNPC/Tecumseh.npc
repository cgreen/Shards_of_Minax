  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>I walk the path of the guardian, a sentinel of these lands.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
