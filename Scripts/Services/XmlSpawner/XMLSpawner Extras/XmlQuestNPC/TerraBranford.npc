  <SpeechEntry>
    <EntryNumber>10</EntryNumber>
    <ID>10</ID>
    <Text>My heart is uneasy, but my spirit is unbroken.</Text>
    <Keywords>health</Keywords>
    <DependsOn>-1</DependsOn>
