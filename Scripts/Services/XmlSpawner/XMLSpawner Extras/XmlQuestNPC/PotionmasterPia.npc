  <SpeechEntry>
    <EntryNumber>0</EntryNumber>
    <ID>0</ID>
    <Text>I am Potionmaster Pia, the alchemist extraordinaire!</Text>
    <Keywords>name</Keywords>
    <DependsOn>-1</DependsOn>
