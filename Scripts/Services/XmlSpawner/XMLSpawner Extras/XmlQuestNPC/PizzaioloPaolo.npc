  <SpeechEntry>
    <EntryNumber>10</EntryNumber>
    <ID>10</ID>
    <Text>Feast your eyes upon my culinary creations! They are a feast for the senses!</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
