  <SpeechEntry>
    <EntryNumber>20</EntryNumber>
    <ID>20</ID>
    <Text>I am an archer, skilled in the art of precision and swiftness.</Text>
    <Keywords>job</Keywords>
    <DependsOn>-1</DependsOn>
